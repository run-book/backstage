  },
  "devDependencies": {
    "@types/node-fetch": "2.6.2",
    "typescript": "^4.0.5",
    "@types/node": "^14.14.7",
    "@types/jest": "^26.0.15",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/user-event": "^12.2.0",
    "jest": "^26.6.0",
    "ts-jest": "^26.4.4"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/"
  ],
  "license": "MIT",
  "repository": "git@github.com:phil-rice/laoban.git",
