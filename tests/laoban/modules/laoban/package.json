{
  "name":            "laoban",
  "description":     "A cli for managing projects that have many npm packages",
  "version":         "1.4.31",
  "main":            "dist/index",
  "types":           "dist/index",
  "bin":             {
    "laoban": "dist/laoban.js"
  },
  "backstage":       {
    "techdocs": "."
  },
  "browserslist":    {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production":  [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies":    {
    "fs-extra":                "^9.0.1",
    "commander":               "^10.0.0",
    "@laoban/generations":     "1.4.31",
    "@laoban/cli":             "1.4.31",
    "@laoban/comparefiles":    "1.4.31",
    "@laoban/config":          "1.4.31",
    "@laoban/debug":           "1.4.31",
    "@laoban/fast-xml-parser": "1.4.31",
    "@laoban/filesops-node":   "1.4.31",
    "@laoban/node":            "1.4.31",
    "@laoban/validation":      "1.4.31",
    "@laoban/variables":       "1.4.31"
  },
  "devDependencies": {
    "@types/fs-extra":             "^9.0.5",
    "typescript":                  "^4.0.5",
    "@types/node":                 "^14.14.7",
    "@types/jest":                 "^26.0.15",
    "@testing-library/jest-dom":   "^5.11.5",
    "@testing-library/user-event": "^12.2.0",
    "jest":                        "^26.6.0",
    "ts-jest":                     "^26.4.4"
  },
  "files":           [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/"
  ],
  "keywords":        [
    "monorepo",
    "devops"
  ],
  "license":         "MIT",
  "repository":      "git@github.com:phil-rice/laoban.git",
  "scripts":         {
    "test":       "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch"
  }
}