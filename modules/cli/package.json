{
  "name": "@runbook/backstage",
  "description": "A cli for backstage tools",
  "version": "0.2.8",
  "main": "dist/index",
  "types": "dist/index",
  "bin": {
    "backstage": "dist/index.js"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "commander": "^10.0.0",
    "@laoban/utils": "1.4.40",
    "@laoban/fileops": "1.4.40",
    "@laoban/filesops-node": "1.4.40",
    "@laoban/variables": "1.4.40",
    "xml2js": "^0.6.2",
    "js-yaml": "^4.1.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.5",
    "@types/xml2js": "^0.4.14",
    "@types/js-yaml": "4.0.9",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2",
    "jest": "29.1.1",
    "ts-jest": "^29.1.1",
    "@types/jest": "29.1.1",
    "@types/node": "^18.0.0",
    "npm-check-updates": "^10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/"
  ],
  "keywords": [
    "github",
    "git",
    "backstage",
    "devops",
    "runbook"
  ],
  "license": "MIT",
  "repository": "git@github.com:run-book/runbook.git",
  "scripts": {
    "test": "jest --config jest.config.json --env node",
    "test:watch": "npm run test -- --watch"
  }
}